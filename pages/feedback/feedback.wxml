<!--pages/feedback/feedback.wxml-->
<view class="section">
  <form bindsubmit="bindFormSubmit">
    <textarea class="feedback" bindblur="bindTextAreaBlur" auto-focus placeholder="请输入您的反馈..." name="textarea" maxlength="-1" />
    <button class="feedbackbtn" form-type="submit" type="primary" open-type="feedback"> 提交 </button>
  </form>
</view>
